   "source": [
    "def gcd(a, b):\n",
    "    if a < b:\n",
    "        raise ValueError('a can not be smaller than b')\n",
    "    x = a\n",
    "    y = b\n",
    "    r = x % y\n",
    "    if (r == 0):\n",
    "        return b\n",
    "    else:\n",
    "        x = y\n",
    "        y = r\n",
    "        return gcd(x, y)"
   ]
